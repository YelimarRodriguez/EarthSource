<h1>Edit Your Profile!</h1>
<%= form_for current_user, url: profile_path, :html => {:multipart => true} do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
    <%= f.file_field :pic %>
    <%= f.hidden_field :pic_cache %>
    <%= f.submit %>
<% end %>